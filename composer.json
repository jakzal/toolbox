{
    "name": "zalas/toolbox",
    "description": "Helps to discover and install tools",
    "type": "project",
    "require": {
        "php": "~8.2.0 || ~8.3.0 || ~8.4.0",
        "symfony/console": "^6.4.18 || ^7.2",
        "psr/container": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5.9 || ^12.0",
        "zalas/phpunit-globals": "^4.0",
        "infection/infection": "^0.29",
        "deptrac/deptrac": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Zalas\\Toolbox\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zalas\\Toolbox\\Tests\\": "tests"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jakub Zalas",
            "email": "jakub@zalas.pl"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
